{
  "image_generation": {
    "model": "dall-e-3",
    "size": "1536x1024",
    "quality": "hd",
    "style": "vivid",
    "max_retries": 2,
    "timeout": 60,
    "cost_per_image": 0.080
  },
  "agents": {
    "default_model": "gpt-4",
    "max_concurrent_agents": 3,
    "agent_timeout": 180,
    "retry_attempts": 2
  },
  "security": {
    "rate_limit_per_minute": 30,
    "max_requests_per_hour": 500,
    "enable_request_logging": true,
    "cors_origins": ["http://localhost:3000", "http://localhost:5002", "http://localhost:5003"]
  },
  "database": {
    "database_url": "sqlite:///dev_campaigns.db",
    "enable_analytics": true,
    "retention_days": 30,
    "backup_enabled": false
  }
}
